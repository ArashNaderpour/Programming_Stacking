<Window x:Class="SpaceStacker.ViewportImageSubWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SpaceStacker"
        mc:Ignorable="d"
        Title="3D Viewport" Height="1200" Width="1500" Icon="/Resources/SpaceStacker_Icon.ico">
    <Grid>
        <Image HorizontalAlignment="Stretch" Margin="10 10 10 10">
            <Image.Source>
                <BitmapImage UriSource="Images/3DViewport.JPG"/>
            </Image.Source>
        </Image>
    </Grid>
</Window>
